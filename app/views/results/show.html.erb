</div>

<script type="text/javascript">


var jeffers;

if (document.getElementById("walking").checked = true) {
	jeffers = google.maps.TravelMode.WALKING;
}
else if (document.getElementById("transit").checked = true){
	jeffers = google.maps.TravelMode.TRANSIT;
console.log(jeffers);
}
// if $('input[value="walking"]') {
// 	jeffers = google.maps.TravelMode.WALKING
// }
// else {
// 	jeffers = google.maps.TravelMode.TRANSIT
// }

// if (document.getElementsByName('mode') == document.)

var directionsDisplay = new google.maps.DirectionsRenderer();
var directionsService = new google.maps.DirectionsService();

function calcRoute() {
	var origin = new google.maps.LatLng(<%= @property.latitude %>, <%= @property.longitude %>);
	var destination = new google.maps.LatLng(45.5200,-122.6819);
	var request = {
		origin: origin,
		destination: destination,
		travelMode: jeffers
	};
	directionsService.route(request, function(response, status){
		if (status == google.maps.DirectionsStatus.OK) {
			directionsDisplay.setDirections(response);
		}
	});
}

calcRoute();

handler = Gmaps.build('Google');
handler.buildMap({ internal: {id: 'directions'}}, function(){
  directionsDisplay.setMap(handler.getMap());
});
    
</script>